SPRING_APPLICATION_NAME=user-service
SERVER_PORT=2000
SPRINGDOC_SWAGGER_UI_PATH=/swagger-ui.html
SPRING_API_DOCS_PATH=/v3/api-docs
SPRING_JPA_HIBERNATE_DDL_AUTO=create
SPRING_JPA_SHOW_SQL=false
SPRING_JPA_OPEN_IN_VIEW=true
OPENTRACING_JAEGER_ENABLE=true
OPENTRACING_JAEGER_SERVICE_NAME=user-service
OPENTRACING_JAEGER_UDP_SENDER_HOST=jaeger
OPENTRACING_JAEGER_UDP_SENDER_PORT=6831
OPENTRACING_JAEGER_LOG_SPANS=true
SPRING_DATASOURCE_URL=jdbc:h2:mem://localhost/~/user-service
SPRING_DATASOURCE_DRIVERCLASSNAME=org.h2.Driver
SPRING_DATASOURCE_USERNAME=sa
SPRING_DATASOURCE_PASSWORD=password
SPRING_JPA_DATABASE_PLATFORM=org.hibernate.dialect.H2Dialect
SPRING_H2_CONSOLE_ENABLED=true
RESILIENCE4J_SLIDING_WINDOWS_TYPE=count_based
RESILIENCE4J_SLIDING_WINDOWS_SIZE=10
RESILIENCE4J_MINIMUM_NUMBER_OF_CALLS=5
RESILIENCE4J_PERMITTED_NUMBER_OF_CALLS_IN_HALFOPENSTATE=3
RESILIENCE4J_WAITDURATIONINOPENSTATE=5s
RESILIENCE4J_FAILURERATETHRESHOLD=50
RESILIENCE4J_REGISTERHEALTHINDICATOR=true
RESILIENCE4J_EVENTCONSUMERBUFFERSIZE=10
RESILIENCE4J_AUTOMITCTRANSITIONFROM=true
CIRCUITBREAKERS_ENABLED=true
EXPOSURE_INCLUDE=health
HEALTH_SHOW_DETAILS=always
